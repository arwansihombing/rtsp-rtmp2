APP_NAME="RTSP to RTMP Gateway"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=rtsp_gateway
DB_USERNAME=rtsp_user
DB_PASSWORD=rtsp_password

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database
SESSION_DRIVER=file
SESSION_LIFETIME=120

# FFmpeg Configuration
FFMPEG_THREADS=4
FFMPEG_PRESET=ultrafast
FFMPEG_TUNE=zerolatency

# Resolution Presets (bitrate in kbps)
# Format: width:height:bitrate:maxrate:bufsize
RESOLUTION_480P=854:480:1000:1000:2000
RESOLUTION_720P=1280:720:1500:1500:3000
RESOLUTION_1080P=1920:1080:3000:3000:6000
RESOLUTION_1440P=2560:1440:6000:6000:12000
RESOLUTION_4K=3840:2160:12000:12000:24000